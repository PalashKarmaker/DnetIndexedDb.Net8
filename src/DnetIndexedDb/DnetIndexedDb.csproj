<Project Sdk="Microsoft.NET.Sdk.Razor">

	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<Authors>Adrian Mustelier Camino</Authors>
		<Company>Datalnet Adrian Mustelier Camino</Company>
		<Description>IndexedDB wrapper for Blazor</Description>
		<LangVersion>preview</LangVersion>
		<PackageProjectUrl>https://github.com/amuste/DnetIndexedDb</PackageProjectUrl>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<RepositoryUrl>https://github.com/amuste/DnetIndexedDb</RepositoryUrl>
		<RepositoryType>Git</RepositoryType>
		<PackageTags>Blazor IndexedDB Datalnet</PackageTags>
		<Copyright>Copyright 2020 (c) Datalnet. All rights reserved.</Copyright>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<Version>2.4.1</Version>
		<PackageReleaseNotes>
			Feature: Added Fluent by Kylar
		</PackageReleaseNotes>
	</PropertyGroup>


	<ItemGroup>
		<PackageReference Include="Microsoft.AspNetCore.Components" Version="8.0.0-rc.1.23421.29" />
		<PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="8.0.0-rc.1.23421.29" />
	</ItemGroup>

</Project>
